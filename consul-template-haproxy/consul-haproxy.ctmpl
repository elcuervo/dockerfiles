global
    maxconn 1024
    user haproxy
    group haproxy
    pidfile /var/run/haproxy.pid

defaults
    mode http
    timeout client 5000
    timeout client 50000

listen http-in
    bind *:80
    option forwardfor
    balance roundrobin
    {{range service "webapp"}}
    server {{.Node}} {{.Address}}:{{.Port}}
    {{end}}
